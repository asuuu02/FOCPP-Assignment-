   "source": [
    "user_input = input(\"Enter temperature in Celsius (e.g. 25C): \")\n",
    "\n",
    "celsius_str = user_input.rstrip('Cc')\n",
    "\n",
    "celsius = float(celsius_str)\n",
    "\n",
    "fahrenheit = (celsius * 9/5) + 32\n",
    "\n",
    "print(f\"{fahrenheit}F\")"
   ]
